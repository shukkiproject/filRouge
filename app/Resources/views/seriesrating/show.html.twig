{% extends 'base.html.twig' %}

{% block body %}
    <h1>SeriesRating</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ seriesRating.id }}</td>
            </tr>
            <tr>
                <th>Ratings</th>
                <td>{{ seriesRating.ratings }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('seriesrating_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('seriesrating_edit', { 'id': seriesRating.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
